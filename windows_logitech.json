{
  "title": "Logitech on Windows",
  "rules": [

    { "description": "App Switcher", "manipulators": [

      {"type": "basic",
          "from": {"key_code": "tab","modifiers": {"mandatory": ["left_option"]}},
          "to": [{"key_code": "tab","modifiers": ["left_command"]}]},
      {"type": "basic",
          "from": {"key_code": "tab","modifiers": {"mandatory": ["left_command"]}},
          "to": []},

      {"type": "basic",
          "from": {"key_code": "left_arrow","modifiers": {"mandatory": ["left_option"]}},
          "to": [{"key_code": "left_arrow","modifiers": ["left_command"]}]},
      {"type": "basic",
          "from": {"key_code": "left_arrow","modifiers": {"mandatory": ["left_command"]}},
          "to": []},
      {"type": "basic",
          "from": {"key_code": "right_arrow","modifiers": {"mandatory": ["left_option"]}},
          "to": [{"key_code": "right_arrow","modifiers": ["left_command"]}]},
      {"type": "basic",
          "from": {"key_code": "right_arrow","modifiers": {"mandatory": ["left_command"]}},
          "to": []}

    ]},

    { "description": "Clipboard", "manipulators": [
      {"type": "basic",
          "from": {"key_code": "x","modifiers": {"mandatory": ["left_control"]}},
          "to": [{"key_code": "x","modifiers": ["left_command"]}]},
      {"type": "basic",
          "from": {"key_code": "x","modifiers": {"mandatory": ["left_command"]}},
          "to": []},
      {"type": "basic",
          "from": {"key_code": "c","modifiers": {"mandatory": ["left_control"]}},
          "to": [{"key_code": "c","modifiers": ["left_command"]}]},
      {"type": "basic",
          "from": {"key_code": "c","modifiers": {"mandatory": ["left_command"]}},
          "to": []},
      {"type": "basic",
          "from": {"key_code": "v","modifiers": {"mandatory": ["left_control"]}},
          "to": [{"key_code": "v","modifiers": ["left_command"]}]},
      {"type": "basic",
          "from": {"key_code": "v","modifiers": {"mandatory": ["left_command"]}},
          "to": []}
    ]},

  { "description": "Symbols", "manipulators": [

      {"description": "@", "type": "basic",
          "from": {"key_code": "v","modifiers": {"mandatory": ["left_option"]}},
          "to": [{"key_code": "2","modifiers": ["left_shift","left_option"]}]},
      {"type": "basic",
          "from": {"key_code": "2","modifiers": {"mandatory": ["left_shift","left_option"]}},
          "to": []},

      {"description": "[", "type": "basic",
          "from": {"key_code": "f","modifiers": {"mandatory": ["left_option"]}},
          "to": [{"key_code": "š","modifiers": ["left_option"]}]},
      {"type": "basic",
          "from": {"key_code": "š","modifiers": {"mandatory": ["left_option"]}},
          "to": []},

      {"description": "]", "type": "basic",
          "from": {"key_code": "g","modifiers": {"mandatory": ["left_option"]}},
          "to": [{"key_code": "đ","modifiers": ["left_option"]}]},
      {"type": "basic",
          "from": {"key_code": "đ","modifiers": {"mandatory": ["left_option"]}},
          "to": []},

      {"description": "{", "type": "basic",
          "from": {"key_code": "b","modifiers": {"mandatory": ["left_option"]}},
          "to": [{"key_code": "š","modifiers": ["left_shift","left_option"]}]},
      {"type": "basic",
          "from": {"key_code": "š","modifiers": {"mandatory": ["left_shift","left_option"]}},
          "to": []},

      {"description": "}", "type": "basic",
          "from": {"key_code": "n","modifiers": {"mandatory": ["left_option"]}},
          "to": [{"key_code": "đ","modifiers": ["left_shift","left_option"]}]},
      {"type": "basic",
          "from": {"key_code": "đ","modifiers": {"mandatory": ["left_shift","left_option"]}},
          "to": []},

      {"description": "|", "type": "basic",
          "from": {"key_code": "w","modifiers": {"mandatory": ["left_option"]}},
          "to": [{"key_code": "ž","modifiers": ["left_shift","left_option"]}]},
      {"type": "basic",
          "from": {"key_code": "ž","modifiers": {"mandatory": ["left_shift","left_option"]}},
          "to": []},

      {"description": "~", "type": "basic",
          "from": {"key_code": "1","modifiers": {"mandatory": ["left_option"]}},
          "to": [{"key_code": "n","modifiers": ["left_option"]}]},

      {"description": "`", "type": "basic",
          "from": {"key_code": "7","modifiers": {"mandatory": ["left_option"]}},
          "to": [{"key_code": "<","modifiers": ["left_option"]}]}

    ]}





  ]
}
